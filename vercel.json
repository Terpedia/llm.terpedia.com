{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.7"
    }
  },
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "/api/info"
    },
    {
      "src": "/responses",
      "methods": ["POST"],
      "dest": "/api/responses"
    }
  ]
}
